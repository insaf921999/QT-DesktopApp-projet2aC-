    <property name="styleSheet">
     <string notr="true">  QTabWidget::pane { /* The tab widget frame */
         border-top: 2px solid rgb(231, 42, 109);
     }

     QTabWidget::tab-bar {
         left: 5px; /* move to the right by 5px */
     }
 /* Style the tab using the tab sub-control. Note that
         it reads QTabBar _not_ QTabWidget */
     QTabBar::tab {
         background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                     stop: 0  background-color:darkCyan, stop: 0.4  rgb(231, 42, 109),
                                     stop: 0.5 rgb(231, 42, 109), stop: 1.0 #D3D3D3);
         border: 2px solid  rgb(231, 42, 109);
         border-bottom-color: rgb(231, 42, 109); /* same as the pane color */
         border-top-left-radius: 4px;
         border-top-right-radius: 4px;
         min-width: 8ex;
         padding: 2px;
     }

     QTabBar::tab:selected, QTabBar::tab:hover {
         background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                     stop: 0 rgb(231, 42, 109), stop: 0.4 rgb(231, 42, 109),
                                     stop: 0.5 rgb(231, 42, 109), stop: 1.0 rgb(231, 42, 109));}

     QTabBar::tab:selected {
         border-color: rgb(231, 42, 109);
         border-bottom-color: rgb(231, 42, 109); /* same as pane color */
     }

     QTabBar::tab:!selected {
         margin-top: 5px; /* make non-selected tabs look smaller */
     }

     /* make use of negative margins for overlapping tabs */
     QTabBar::tab:selected {
         /* expand/overlap to the left and right by 4px */
         margin-left: -4px;
         margin-right: -4px;
     }

     QTabBar::tab:first:selected {
         margin-left: 0; /* the first selected tab has nothing to overlap with on the left */
     }

     QTabBar::tab:last:selected {
         margin-right: 0; /* the last selected tab has nothing to overlap with on the right */
     }

     QTabBar::tab:only-one {
         margin: 0; /* if there is only one tab, we don't want overlapping margins */
     }</string>
